<?xml version="1.0"?>
<robot name="Greyhound">
<!-- TORSO -->
  <link name="torso">
    <visual>
      <geometry>
        <box size="0.4 0.15 0.1"/>
      </geometry>
      <material name="gray">
        <color rgba="0.6 0.6 0.6 1"/>
      </material>
    </visual>
    <collision>
    <origin xyz="0 0 0"/>
    <geometry>
      <box size="0.3 0.15 0.1"/>
    </geometry>
  </collision>
<inertial>
    <origin xyz="0 0 0"/>
    <mass value="0.7"/>
    <inertia ixx="0.0001" iyy="0.0001" izz="0.0001"
             ixy="0" ixz="0" iyz="0"/>
</inertial> 
  </link>
<!-- FRONT RIGHT THIGH -->
  <link name="fr_thigh">
   <visual>
     <origin xyz="0 0 -0.075"/> 
     <geometry>
        <box size="0.05 0.05 0.15"/>
     </geometry>
     <material name="light_gray">
     <color rgba="0.7 0.7 0.7 1"/>
     </material>
   </visual>
   <collision>
    <origin xyz="0 0 -0.075"/>
    <geometry>
      <box size="0.05 0.05 0.15"/>
    </geometry>
  </collision>
<inertial>
    <origin xyz="0 0 0"/>
    <mass value="0.2"/>
    <inertia ixx="0.0001" iyy="0.0001" izz="0.0001"
             ixy="0" ixz="0" iyz="0"/>
</inertial> 
  </link>
<!--FRONT RIGHT SHIN -->
  <link name="fr_shin"> 
