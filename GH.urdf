<!-- FR HIP JOINT: torso → fr_thigh -->
  <joint name="fr_hip" type="revolute">
    <parent link="torso"/>
    <child link="fr_thigh"/>
    <origin xyz="0.1 -0.075 0" rpy="0 0 0"/> 
    <axis xyz="0 1 0"/> 
    <limit lower="-1.57" upper="1.57" effort="10" velocity="5"/>
  </joint>
<!-- FR KNEE JOINT: fr_thigh → fr_shin -->
  <joint name="fr_knee" type="revolute">
    <parent link="fr_thigh"/>
    <child link="fr_shin"/>
    <origin xyz="0 0 -0.075" rpy="0 0 0"/> 
    <axis xyz="0 1 0"/> <!-- same axis; you can flip this to reverse bend -->
    <limit lower="-1.57" upper="1.57" effort="10" velocity="5"/>
  </joint>
<!-- FL HIP JOINT: torso → fl_thigh -->
  <joint name="fl_hip" type="revolute">
    <parent link="torso"/>
    <child link="fl_thigh"/>
    <origin xyz="0.1 0.075 0" rpy="0 0 0"/> 
